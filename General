import socket

server = "ofs-nonprod-sql-server-text-analysis.database.windows.net"
port = 1433

sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
sock.settimeout(5)  # Timeout in 5 seconds

try:
    sock.connect((server, port))
    print("✅ Port 1433 is OPEN. The issue is not a blocked port.")
except Exception as e:
    print(f"❌ Port 1433 is BLOCKED. Error: {e}")
finally:
    sock.close()
